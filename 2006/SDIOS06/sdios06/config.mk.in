# @configure_input@

ARCH=		@ARCH@
PLAT=           @PLAT@

prefix=		@prefix@
exec_prefix=	@exec_prefix@
includedir=	@includedir@
libdir=		@libdir@
libexecdir=	@prefix@
kerneldir=	@kerneldir@
idldir=		@idldir@

@SET_MAKE@
SHELL=		@SHELL@
CC=		@CC@
CXX=		$(CC) -x c++
AS=		$(CC)
CFLAGS=		@CFLAGS@
CXXFLAGS=	@CXXFLAGS@
LDFLAGS=	@LDFLAGS@
CPPFLAGS=	@CPPFLAGS@
IDLFLAGS=       @IDLFLAGS@ --word-size=@BITS@ --interface=V4 --platform=generic
LGCC=		-lgcc

TOOLPREFIX=	@TOOLPREFIX@
AR=		$(TOOLPREFIX)ar
RANLIB=		$(TOOLPREFIX)ranlib
LD=		$(TOOLPREFIX)ld
RMDIR=		rmdir
LN_S=		@LN_S@
AUTOCONF=	autoconf
MKDIRHIER=	$(top_srcdir)/tools/mkdirhier
AWK=		@AWK@
IDL=		@IDL@

INSTALL=	@INSTALL@
INSTALL_PROGRAM=@INSTALL_PROGRAM@
INSTALL_DATA=	@INSTALL_DATA@
INSTALL_SCRIPT=	@INSTALL_SCRIPT@

KICKSTART_LINKBASE=	@KICKSTART_LINKBASE@
SIGMA0_LINKBASE=	@SIGMA0_LINKBASE@
ROOTTASK_LINKBASE=	@ROOTTASK_LINKBASE@
